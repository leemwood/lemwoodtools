<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- 
    This file contains suppressions for known false positives in dependency check.
    Add suppressions here for vulnerabilities that don't apply to your specific use case.
    
    Example suppression:
    <suppress>
        <notes><![CDATA[
        This vulnerability doesn't apply to our use case because...
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.example/.*@.*$</packageUrl>
        <cve>CVE-2021-12345</cve>
    </suppress>
    -->
    
    <!-- Suppress false positives for Android SDK components -->
    <suppress>
        <notes><![CDATA[
        Android SDK components are managed by Google and updated through the SDK manager.
        These CVEs typically don't apply to Android app development.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.android\..*@.*$</packageUrl>
    </suppress>
    
    <!-- Suppress false positives for AndroidX libraries -->
    <suppress>
        <notes><![CDATA[
        AndroidX libraries are maintained by Google and follow their security practices.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/androidx\..*@.*$</packageUrl>
    </suppress>
</suppressions>